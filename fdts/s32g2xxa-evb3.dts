/dts-v1/;
#include "s32g.dtsi"
#include "s32g2.dtsi"
#include "s32gxxxa-evb.dtsi"

/ {
	model = "NXP S32G2XXX-EVB3";
};

&wkpu {
	status = "okay";
};

&qspi {
	status = "okay";
};

&pcie0 {
	status = "okay";
};

&pcie1 {
	status = "okay";
};

&serdes0 {
	status = "okay";
};

&serdes1 {
	status = "okay";
};

&pfe_mdio2 {
	status = "okay";
	/* AQR113 on S32G-PROCEVB3-S */
	mdio_a_phy8: ethernet-phy@8 {
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <8>;
	};
};

&pfe_netif0 {
	status = "okay";
	phy-mode = "sgmii";
	phy-handle = <&mdio_a_phy8>;
	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&pfe_netif1 {
	status = "okay";
};

&pfe_netif2 {
	status = "okay";
};
